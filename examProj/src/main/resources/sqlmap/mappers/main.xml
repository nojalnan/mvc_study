<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exam.proj.main.service.impl.MainMapper">

	<select id="getBoard" resultType="mainVo" parameterType="_long">
		select
		binum, bititle, bicontent, bipwd, creusr, credat from board_info
		where
		binum = #{binum}
	</select>

	<select id="getListResult" resultType="mainVo">
		SELECT * FROM board_info
	</select>
</mapper>